{
  "name": "jspm",
  "description": "Browser package management",
  "license": "Apache-2.0",
  "version": "3.0.0-beta",
  "scripts": {
    "build-api": "tsc -p . && cp src/*.js lib/ && rollup --plugin @rollup/plugin-commonjs lib/api.js -d dist",
    "build-node": "ncc build -t src/node.js -e sver -s -e es-module-lexer",
    "build-node-production": "ncc build -t -m src/node.js -e sver -e es-module-lexer",
    "watch-build-node": "ncc build -t src/node.js -s -e sver -e es-module-lexer -w",
    "watch-build-node-production": "ncc build -t -m src/node.js -e sver -e es-module-lexer -w",
    "build": "npm run build-node && npm run build-api",
    "test": "node test/test.js",
    "unit": "node src/script-lexer.js",
    "generate-maps": "node test/generate-maps.mjs"
  },
  "exports": {
    "publish": "./dist/index.js",
    "default": "./src/cli.ts"
  },
  "bin": {
    "jspm": "dist/index.js"
  },
  "main": "./lib/tracemap.js",
  "dependencies": {
    "es-module-lexer": "0.3.26",
    "sver": "^1.8.3",
    "@iarna/toml": "^3.0.0",
    "mkdirp": "^1.0.4"
  },
  "devDependencies": {
    "cache": "file:./deps/cache",
    "chalk": "^4.1.0",
    "make-fetch-happen": "^8.0.3",
    "ora": "^5.1.0",
    "rimraf": "^3.0.2"
  },
  "files": [
    "bin",
    "lib"
  ]
}
